<h2>Edit Patient</h2>

<%= form_for @patient do |f| %>
	<%= f.label :first_name %>
	<%= f.text_field :first_name %>
<br>
	<%= f.label :last_name %>
	<%= f.text_field :last_name %>
<br>
	<%= f.label :DOB %>
	<%= f.text_field :DOB %>
<br>
	<%= f.label :gender %>
	<%= f.text_field :gender %>
<br>
	<%= f.label :description %>
	<%= f.text_field :description %>
<br>
	<%= f.submit %>

<% end %>