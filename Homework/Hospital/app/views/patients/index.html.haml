%h1 Hospital Patient Status
= link_to "Create New Patient", new_patient_path
%br/
= link_to "Home", root_path
- @patient.each do |patient|
  = patient.first_name
  = patient.last_name
  = patient.dob
  = patient.gender
  = patient.description
  %strong= patient.workflow_state
  = link_to "Edit", edit_patient_path(patient.id)
  = link_to "Delete", patient_path(patient), method: :delete
  %br/
