%h1 Anna's Hospital
= link_to "Create New Patient", new_patient_path
%br/
- @patient.each do |patient|
  = patient.first_name
  = patient.last_name
  = patient.dob
  = patient.gender
  = patient.description
  = link_to "Edit", edit_patient_path(patient.id)
  = link_to "Delete", patient_path(patient), method: :delete
  %br/
